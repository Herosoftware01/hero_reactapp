<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <staticContent>
            <mimeMap fileExtension=".webmanifest" mimeType="application/manifest+json" />
        </staticContent>
         <httpProtocol>
      <customHeaders>
        <add name="Service-Worker-Allowed" value="/" />
